              xmlns:ns0="http://etnetera.com/projects/datart/bambino">
    <env:Body>
        <env:Fault xsi:type="env:Fault">
            <faultcode>env:Server</faultcode>
            <faultstring>com.etnetera.projects.datart.ws.gen.RequestError</faultstring>
            <detail>
                <ns0:requestError>
                    <errorCode>ns0:BadArguments</errorCode>
