<?xml version="1.0" encoding="UTF-8"?>
<!--
* E-SHOP BAMBINO
*
* WSDL specifikace pro komunikaci e-shopu s ERP systemem.
*
* Tento soubor obsahuje popis dvou portu webovych sluzeb:
*
* BambinoEshopUpdate: sluzby poskytovane e-shopem a volane z ERP,
* tedy zejmena aktualizaci katalogu.
*
* BambinoErpUpdate: sluzby poskytovane ERP a volane z e-shopu,
* v soucasnosti pouze aktualizace objednavek a zakazniku.
*
* Autor (retired): Martin Kacer, martin.kacer@etnetera.cz
* Autor (retired): Martin Varecha, martin.varecha@etnetera.cz
* Autor (retired): Pavel Sintak, pavel.sintak@etnetera.cz
* Autor: Pavel Vodrazka, pavel.vodrazka@etnetera.cz
* Autor (retired): Michal Kreuzman, michal.kreuzman@etnetera.cz
* Autor(retired):Adam Zivner, adam.zivner@etnetera.cz
* Autor(retired): Bohumir Zamecnik, bohumir.zamecnik@etnetera.cz
* Autor: Lukas Blail, lukas.blail@etnetera.cz
* Verze: 1.60.1 (validace DIC, pinStatus)
-->

<wsdl:definitions name="BambinoUpdate"
	targetNamespace="http://etnetera.com/projects/datart/bambino"
	xmlns:impl="http://etnetera.com/projects/datart/bambino"
	xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">


<!-- *************** definice typu *************** -->

<wsdl:types>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://etnetera.com/projects/datart/bambino">

	<!-- pole stringu, pouzito pro hodnoty vyctoveho typu a keywords -->
	<complexType name="StringArray">
		<sequence>
			<element name="eString" type="string" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- obecna chyba zpracovani requestu -->
	<element name="requestError">
		<complexType>
			<sequence>
				<element name="errorCode" type="xsd:QName" minOccurs="1" maxOccurs="unbounded"/>
				<element name="errorText" minOccurs="0" maxOccurs="unbounded">
					<complexType>
						<simpleContent>
							<extension base="xsd:string">
								<attribute name="lang" type="NMTOKEN"/>
							</extension>
						</simpleContent>
					</complexType>
				</element>
			</sequence>
		</complexType>
	</element>

	<!-- signalizace poruseni referencni integrity -->
	<complexType name="EntityReference">
		<sequence>
			<element name="entityType" type="xsd:string"/>
			<element name="entityId" type="impl:ErpId"/>
		</sequence>
	</complexType>
	<element name="referentialIntegrityError">
		<complexType>
			<sequence>
				<element name="missingEntities" type="impl:EntityReference" minOccurs="1" maxOccurs="unbounded"/>
			</sequence>
		</complexType>
	</element>

	<!-- seznam cisel verzi udrzovanych pro vsechny typy entit -->
	<complexType name="VersionList">
		<sequence>
			<element name="verAttributes" minOccurs="0" type="xsd:long"/>
			<element name="verAvailability" minOccurs="0" type="xsd:long"/>
			<element name="verCatalogue" minOccurs="0" type="xsd:long"/>
			<element name="verCategories" minOccurs="0" type="xsd:long"/>
			<element name="verComplaints" minOccurs="0" type="xsd:long"/>
			<element name="verOrders" minOccurs="0" type="xsd:long"/>
			<element name="verPrices" minOccurs="0" type="xsd:long"/>
			<element name="verProducers" minOccurs="0" type="xsd:long"/>
			<element name="verRelations" minOccurs="0" type="xsd:long"/>
			<element name="verCommodityFamilies" minOccurs="0" type="xsd:long"/>
			<element name="verCommoditySegments" minOccurs="0" type="xsd:long"/>
			<element name="verProductLines" minOccurs="0" type="xsd:long"/>
			<element name="verSellGroups" minOccurs="0" type="xsd:long"/>
			<element name="verSlotForRegion" minOccurs="0" type="xsd:long"/>
			<element name="verDeliveryRegion" minOccurs="0" type="xsd:long"/>
			<element name="verMicroRegion" minOccurs="0" type="xsd:long"/>
			<element name="verZipCode" minOccurs="0" type="xsd:long"/>
			<element name="verTimeSlotDate" minOccurs="0" type="xsd:long"/>
			<element name="verTimeSlotPricingDetail" minOccurs="0" type="xsd:long"/>
			<element name="verEANVoucher" minOccurs="0" type="xsd:long"/>
			<element name="verVoucher" minOccurs="0" type="xsd:long"/>
			<element name="verDailySales" minOccurs="0" type="xsd:long"/>
			<element name="verDeliveryDelay" minOccurs="0" type="xsd:long"/>
			<element name="verAttributeGroups" minOccurs="0" type="xsd:long"/>
			<element name="verBranches" minOccurs="0" type="xsd:long"/>
			<element name="verEmployeeWorkingHours" minOccurs="0" type="xsd:long"/>
			<element name="verCommodityAttrValue" minOccurs="0" type="xsd:long"/>
			<element name="verCommodityCategory" minOccurs="0" type="xsd:long"/>
		</sequence>
	</complexType>

	<!-- Stav sluzby -->
	<simpleType name="ServiceState">
		<restriction base="xsd:string">
			<enumeration value="NONE" />
			<enumeration value="ORDERED" />
			<enumeration value="CONTRACTED" />
			<enumeration value="DATE_BOOKED" />
			<enumeration value="AVAILABLE" />
			<enumeration value="COMPLETED" />
			<enumeration value="CANCELLED" />
		</restriction>
	</simpleType>

	<!-- Prubeh sluzby -->
	<simpleType name="ServiceWorkflowValue">
		<restriction base="xsd:string">
			<maxLength value="1"/>
			<pattern value="[A-C]"/>
		</restriction>
	</simpleType>

	<!-- zakladni identifikator pro ruzne pouziti -->
	<simpleType name="Identifier">
		<restriction base="xsd:string">
			<maxLength value="20"/>
			<pattern value="[A-Za-z0-9-][A-Za-z0-9_-]*"/>
		</restriction>
	</simpleType>
	<complexType name="IdentifierArray">
		<sequence>
			<element name="eIdentifier" type="impl:Identifier" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- jednoznacna identifikace entity v ERP systemu -->
	<simpleType name="ErpId">
		<restriction base="impl:Identifier"/>
	</simpleType>
	<complexType name="ErpIdArray">
		<sequence>
			<element name="eErpId" type="impl:ErpId" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- crypto id -->
	<simpleType name="CryptoId">
		<restriction base="xsd:string">
			<maxLength value="32"/>
			<pattern value="[A-Za-z0-9][A-Za-z0-9_-]*"/>
		</restriction>
	</simpleType>

	<!-- string reprezentace binarniho cisla -->
	<simpleType name="BinaryString">
		<restriction base="xsd:string">
			<pattern value="[01]+"/>
		</restriction>
	</simpleType>

	<!-- retezec umoznujici jazykove verze -->
	<complexType name="LangString">
		<sequence>
			<element name="cs" nillable="true" minOccurs="0" type="xsd:string"/>
			<element name="sk" nillable="true" minOccurs="0" type="xsd:string"/>
		</sequence>
	</complexType>
	<complexType name="LangStringArray">
		<sequence>
			<element name="cs" nillable="true" minOccurs="0" type="impl:StringArray"/>
			<element name="sk" nillable="true" minOccurs="0" type="impl:StringArray"/>
		</sequence>
	</complexType>

	<!-- identifikace segmentu -->
	<simpleType name="SegmentCode">
		<restriction base="xsd:string">
			<enumeration value="CZ"/>
			<enumeration value="SK"/>
		</restriction>
	</simpleType>

	<!-- priznaky -->
	<simpleType name="Flag">
		<restriction base="xsd:string">
			<pattern value="[+-][A-Za-z][A-Za-z0-9_-]*"/>
			<maxLength value="21"/>
		</restriction>
	</simpleType>
	<simpleType name="ListOfFlag">
		<!--  <list itemType="impl:Flag"/> -->
		<restriction base="xsd:string">
			<pattern value="[+-][A-Za-z][a-zA-Z0-9_-]*( [+-][A-Za-z][a-zA-Z0-9_-]*)*"/>
		</restriction>
	</simpleType>

	<!-- typ dostupnosti -->
	<simpleType name="AvailabilityMode">
		<restriction base="impl:Identifier"/>
	</simpleType>

	<!-- typ hodnoty pro dynamicke atributy -->
	<simpleType name="ValueType">
		<restriction base="xsd:string">
			<enumeration value="VT_INTEGER"/>
			<enumeration value="VT_DECIMAL"/>
			<enumeration value="VT_BOOLEAN"/>
			<enumeration value="VT_STRING"/>
			<enumeration value="VT_ENUM"/>
			<enumeration value="VT_MULTI"/>
		</restriction>
	</simpleType>

	<!-- zpusob urceni ceny -->
	<simpleType name="PriceType">
		<restriction base="xsd:string">
			<enumeration value="PRICE_NORMAL"/>
			<enumeration value="PRICE_GIFT"/>
			<enumeration value="PRICE_DISCOUNT"/>
		</restriction>
	</simpleType>

	<!-- omezeni poctu kusu u prislusenstvi -->
	<simpleType name="AccessoryLimit">
		<restriction base="xsd:string">
			<enumeration value="AL_LIMIT"/>
			<enumeration value="AL_EXACT"/>
			<enumeration value="AL_ANY"/>
		</restriction>
	</simpleType>

	<!-- zpusob mazani vazeb -->
	<simpleType name="RelDeleteMode">
		<restriction base="xsd:string">
			<enumeration value="DEL_NONE"/>
			<enumeration value="DEL_EXACT"/>
			<enumeration value="DEL_FIRST"/>
			<enumeration value="DEL_SECOND"/>
			<enumeration value="DEL_ID"/>
		</restriction>
	</simpleType>

	<!-- role zbozi -->
	<simpleType name="CommodityRole">
		<restriction base="xsd:string">
			<enumeration value="MAIN"/>
			<enumeration value="ACCESSORY"/>
		</restriction>
	</simpleType>

	<!-- platnost a rozsah aktualizace -->
	<complexType name="UpdateScope">
		<sequence>
			<element name="segment" type="impl:SegmentCode"/>
			<element name="oldVersion" type="xsd:long"/>
			<element name="newVersion" type="xsd:long"/>
		</sequence>
	</complexType>


	<!-- ========== entity bez samostatne identifikace ========== -->

	<!-- dostupnost zbozi -->
	<complexType name="Availability">
		<sequence>
			<element name="commodityRef" type="impl:ErpId"/>
			<element name="available" type="impl:AvailabilityMode"/>
			<element name="hidden" minOccurs="0" type="xsd:boolean"/>
			<element name="comment" nillable="true" minOccurs="0" type="xsd:string"/>
			<element name="amount" nillable="true" minOccurs="0" type="xsd:decimal"/>
			<element name="cncAmount" nillable="true" minOccurs="0" type="xsd:decimal"/>
			<element name="shop" nillable="true" minOccurs="0" type="impl:Identifier"/>
			<element name="delay" minOccurs="0" type="xsd:integer"/> <!-- Zpozdeni zavozu pres CS cizi zeme -->
		</sequence>
	</complexType>
	<complexType name="AvailabilityArray">
		<sequence>
			<element name="eAvailability" type="impl:Availability" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- cena zbozi -->
	<complexType name="PriceData">
		<sequence>
			<element name="amount" nillable="true" minOccurs="0" type="xsd:decimal"/>
			<element name="vat" nillable="true" minOccurs="0" type="xsd:int"/>
			<element name="disposalFee" nillable="true" minOccurs="0" type="xsd:decimal"/>
			<element name="disposalVat" nillable="true" minOccurs="0" type="xsd:int"/>
		</sequence>
	</complexType>
	<complexType name="Price">
		<complexContent>
			<extension base="impl:PriceData">
				<sequence>
					<element name="validFrom" nillable="true" minOccurs="0" type="xsd:dateTime"/>
					<element name="validTo" nillable="true" minOccurs="0" type="xsd:dateTime"/>
					<element name="commodityRef" nillable="true" minOccurs="0" type="impl:ErpId"/>
					<element name="shop" nillable="true" minOccurs="0" type="impl:Identifier"/>
					<element name="id" nillable="true" minOccurs="0" type="impl:ErpId"/>
					<element name="specialPrice" nillable="true" minOccurs="0" type="xsd:string"/>
					<!-- Minimalni mnozstvi kusu zbozi pro ktere je cena aplikovatelna. Musi byt vzdy >= 1.
						 Pokud komodita nema cenu s minCommodityAmount = 1, neni komodita prodejna. -->
					<element name="minCommodityAmount" nillable="false" minOccurs="1" maxOccurs="1" type="xsd:int"/>
					<!-- cena pro dealery-->
					<element name="dealerAmount" nillable="true" minOccurs="0" type="xsd:decimal"/>
					<!-- skrtana cena-->
					<element name="originalAmount" nillable="true" minOccurs="0" type="xsd:decimal"/>
				</sequence>
				<attribute name="deleted" type="xsd:boolean" use="optional" default="false"/>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="PriceArray">
		<sequence>
			<element name="ePrice" type="impl:Price" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- atribut typicky pro kategorii -->
	<complexType name="CategoryAttr">
		<sequence>
			<element name="categoryRef" type="impl:ErpId"/>
			<element name="listPos" nillable="true" minOccurs="0" type="xsd:int"/>
			<element name="filterPos" nillable="true" minOccurs="0" type="xsd:int"/>
			<element name="globalPos" nillable="true" minOccurs="0" type="xsd:int"/>
			<element name="groupPos" nillable="true" minOccurs="0" type="xsd:int"/>
			<element name="attributeGroupRef" nillable="true" type="impl:ErpId"/>
		</sequence>
	</complexType>
	<complexType name="CategoryAttrArray">
		<sequence>
			<element name="eCategory" type="impl:CategoryAttr" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- hodnota dynamickeho atributu -->
	<complexType name="AttrValue">
		<sequence>
			<element name="attributeRef" type="impl:ErpId"/>
			<element name="valueNum" nillable="true" minOccurs="0" type="xsd:decimal"/>
			<element name="valueTxt" nillable="true" minOccurs="0" type="impl:LangString"/>
			<element name="valueMulti" nillable="true" minOccurs="0" type="impl:LangStringArray"/>
			<element name="comment" nillable="true" minOccurs="0" type="impl:LangString"/>
		</sequence>
	</complexType>
	<complexType name="AttrValueArray">
		<sequence>
			<element name="eAttrValue" type="impl:AttrValue" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
	<!-- hodnota atributu v samostatnem datovem toku -->
	<complexType name="CommodityAttrValue">
		<complexContent>
			<extension base="impl:AttrValue">
				<sequence>
					<element name="commodityRef" nillable="true" type="impl:ErpId"/>
				</sequence>
			<attribute name="deleted" type="xsd:boolean" use="optional" default="false"/>
			</extension>
		</complexContent>
	</complexType>
	
	<complexType name="CommodityAttrValueArray">
		<sequence>
			<element name="eCommodityAttrValue" type="impl:CommodityAttrValue" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
	<!-- produkt v kategorii -->
	<complexType name="CommodityCategory">
		<sequence>
			<element name="commodityRef" type="impl:ErpId"/>
			<element name="categoryRef"  type="impl:ErpId"/>
		</sequence>
		<attribute name="deleted" type="xsd:boolean" use="optional" default="false"/>
	</complexType>
	
	<complexType name="CommodityCategoryArray">
		<sequence>
			<element name="eCommodityCategory" type="impl:CommodityCategory" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- jednoznacna identifikace entity v ERP systemu + definice platnosti OD - DO -->
	<complexType name="ErpIdValid">
		<sequence>
			<element name="id" type="impl:ErpId"/>
			<element name="validFrom" nillable="true" minOccurs="0" type="xsd:dateTime"/>
			<element name="validTo" nillable="true" minOccurs="0" type="xsd:dateTime"/>
		</sequence>
	</complexType>
	<complexType name="ErpIdValidArray">
		<sequence>
			<element name="eErpIdValid" type="impl:ErpIdValid" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- prodejni skupiny -->
	<complexType name="SellGroup">
		<sequence>
			<element name="key" type="impl:Identifier"/>
			<element name="title" nillable="true" type="xsd:string"/>
			<!--element name="validFrom" nillable="true" minOccurs="0" type="xsd:dateTime"/>
			<element name="validTo" nillable="true" minOccurs="0" type="xsd:dateTime"/ -->
			<element name="commodityRefs" nillable="true" type="impl:ErpIdValidArray"/>
		</sequence>
	</complexType>
	<complexType name="SellGroupArray">
		<sequence>
			<element name="eSellGroup" type="impl:SellGroup" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<complexType name="DeliveryDelay">
		<sequence>
			<element name="deliveryRef" type="impl:ErpId"/>
			<element name="deliveryMode" type="impl:DeliveryMode"/>
			<element name="delayCorrection" type="xsd:integer"/>
		</sequence>
	</complexType>
	<complexType name="DeliveryDelayArray">
		<sequence>
			<element name="eDeliveryDelay" type="impl:DeliveryDelay" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- ========== definice ERP entit ========== -->

	<!-- spolecny predek -->
	<complexType name="ErpObject">
		<sequence>
			<element name="id" type="impl:ErpId"/>
			<element name="created" nillable="true" minOccurs="0" type="xsd:dateTime"/>
		</sequence>
		<attribute name="deleted" type="xsd:boolean" use="optional" default="false"/>
	</complexType>

	<!-- vyrobce -->
	<complexType name="Producer">
		<complexContent>
			<extension base="impl:ErpObject">
				<sequence>
					<element name="title" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="description" nillable="true" minOccurs="0" type="impl:LangString"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ProducerArray">
		<sequence>
			<element name="eProducer" type="impl:Producer" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- kategorie -->
	<complexType name="Category">
		<complexContent>
			<extension base="impl:ErpObject">
				<sequence>
					<element name="parentRef" nillable="true" minOccurs="0" type="impl:ErpId"/>
					<element name="title" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="titleSeo" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="titleHead" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="descriptions" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="keywords" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="urlId" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="aggregated" nillable="true" minOccurs="0" type="xsd:boolean"/>
					<element name="position" nillable="true" minOccurs="0" type="xsd:int"/>
					<element name="heureka" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="categoryRelation" nillable="true" minOccurs="0" type="xsd:string"/>
					<element name="alias" nillable="true" minOccurs="0" type="xsd:string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="CategoryArray">
		<sequence>
			<element name="eCategory" type="impl:Category" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- dynamicky atribut - definice -->
	<complexType name="Attribute">
		<complexContent>
			<extension base="impl:ErpObject">
				<sequence>
					<element name="title" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="description" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="valType" nillable="true" minOccurs="0" type="impl:ValueType"/>
					<element name="valUnit" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="enumList" nillable="true" minOccurs="0" type="impl:LangStringArray"/>
					<element name="inCategories" nillable="true" minOccurs="0" type="impl:CategoryAttrArray"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="AttributeArray">
		<sequence>
			<element name="eAttribute" type="impl:Attribute" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
	<!-- skupina dynamickych  atributu - definice -->
	<complexType name="AttributeGroup">
		<complexContent>
			<extension base="impl:ErpObject">
				<sequence>
					<element name="title" nillable="true" minOccurs="0" type="impl:LangString"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="AttributeGroupArray">
		<sequence>
			<element name="eAttributeGroup" type="impl:AttributeGroup" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

<!-- reklamace -->

<simpleType name="ComplaintResolution">
    	<restriction base="xsd:string">
			<enumeration value="REPAIRED">
				<annotation>
					<documentation>Oprava veci. Tento typ vyrizeni se nabizi i zakaznikovi pri zadani reklamace.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="REPLACED">
				<annotation>
					<documentation>Vymena veci. Tento typ vyrizeni se nabizi i zakaznikovi pri zadani reklamace.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="WITHDRAWN">
				<annotation>
					<documentation>Odstoupeni od smlovy. Tento typ  se nabizi i  pri zadani reklamace.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="FAILRUE_UNDETECTED">
				<annotation>
					<documentation>Zavada se neprojevila - k oprave nedoslo.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="REJECTED">
				<annotation>
					<documentation>Reklamace byla zamitnuta (napr. poruseni zarucnich podminek)</documentation>
				</annotation>
			</enumeration>
			<enumeration value="COMPONENT_REPLACED">
				<annotation>
					<documentation>Opraveno vymenou soucasti zarizeni.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="DISCOUNTED">
				<annotation>
					<documentation>Sleva z ceny zarizeni</documentation>
				</annotation>
			</enumeration>
			<enumeration value="RETURNED">
				<annotation>
					<documentation>Vraceno zakaznikovi bez opravy.</documentation>
				</annotation>
			</enumeration>
    	</restriction>
    </simpleType>
	
	<simpleType name="ComplaintState">
		<restriction base="xsd:string">
			<enumeration value="NEW" >
				<annotation>
					<documentation>Nova reklamace (s timto stavem ESH reklamaci posila do AX pri zalozeni)</documentation>
				</annotation>
			</enumeration>
			<enumeration value="RECEIVED" >
				<annotation>
					<documentation>Reklamace je v AX prijata a dale se zpracovava.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="ACCEPTED" >
				<annotation>
					<documentation>Reklamace byla akceptovana k dalsimu zpracovani. Zacina zakonna lhuta pro vyrizeni.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="SENT_OFF" >
				<annotation>
					<documentation>Odeslano do servisu</documentation>
				</annotation>
			</enumeration>
			<enumeration value="NOT_REPAIRED" >
				<annotation>
					<documentation>Neopraveno - servis zarizeni neprijme kvuli nesrovnalosti pri prevzeti - napr. neni shoda serioveho cisla.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="PRICE_OFFERED" >
				<annotation>
					<documentation>U placene opravy - servis stanovil odhad ceny opravy, je treba ji nabidnout zakaznikovi.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="PRICE_ACCEPTED" >
				<annotation>
					<documentation>Cena opravy byla zakaznikem prijata - servis zbozi opravi</documentation>
				</annotation>
			</enumeration>
			<enumeration value="PRICE_REJECTED" >
				<annotation>
					<documentation>Cena opravy byla zakaznikem zamitnuta - servis zbozi vrati</documentation>
				</annotation>
			</enumeration>
			<enumeration value="PRICE_FINALIZED" >
				<annotation>
					<documentation>Zbozi bylo opraveno a cena opravy byla stanovena na konecnou hodnotu.</documentation>
				</annotation>
			</enumeration>	
			<enumeration value="SENT_BACK" >
				<annotation>
					<documentation>Vraceno ze servisu.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="WAIT_FOR_WITHDRAW" >
				<annotation>
					<documentation>Reklamovane zbozi ceka na vyzvednuti na prodejne.</documentation>
				</annotation></enumeration>
			<enumeration value="REPAIRED" >
				<annotation>
					<documentation>Reklamace vyrizena opravou.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="CANCELED" >
				<annotation>
					<documentation>Reklamace vyrizena zamitnutim.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="CREDITED" >
				<annotation>
					<documentation>Reklamace vyrizena dobropisem.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="RETURNED" >
				<annotation>
					<documentation>Reklamace vyrizena vracenim zbozi.</documentation>
				</annotation></enumeration>
		</restriction>
	</simpleType>	
	
	<simpleType name="ComplaintType">
		<restriction base="xsd:string">
			<enumeration value="3D" >
				<annotation>
					<documentation>Vraceni zbozi do 3 dnu bez udani duvodu. Zbozi musi byt nepouzite (zabalene, ...)</documentation>
				</annotation></enumeration>
			<enumeration value="7D" >
				<annotation>
					<documentation>Vraceni zbozi do 7/14 dni (dle zakona o eshopech)</documentation>
				</annotation>
			</enumeration>
			<enumeration value="WARRANTY" >
				<annotation>
					<documentation>Zarucni reklamace zbozi zakoupeneho na ESH.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="EXTENDED_WARRANTY" >
				<annotation>
					<documentation>Zarucni reklamace zbozi zakoupeneho na ESH.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="FLASH_EXCHANGE_WARRANTY" >
				<annotation>
					<documentation>Zaruka bleskove vymeny</documentation>
				</annotation>
			</enumeration>
			<enumeration value="INSURENCE" >
				<annotation>
					<documentation>Pojistna udalost</documentation>
				</annotation>
			</enumeration>
			<enumeration value="ORDINAL" >
				<annotation>
					<documentation>Pozarucni reklamace (placena oprava) zbozi zakoupeneho na ESH.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="EXTERNAL" >
				<annotation>
					<documentation>Reklamace nebo oprava zbozi koupeneho mimo ESH (pred jeho spustenim)</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

	<complexType name="Complaint">
        <annotation>
        	<documentation>Data pro prenos informaci o reklamacich. Tato struktura se prenasi z eshopu i na eshop, pro kazdy smer jsou relevantni jine sub-elementy. 
        	Reklamace nekdy vaze k jedne nebo vice objednavek - objednavky vznikaji na zaklade aktivit v zivotnim cyklu reklamace.</documentation>
        </annotation>
		<complexContent>
			<extension base="impl:ErpObject">
        <sequence>
			<element name="userRef" type="impl:Identifier" minOccurs="0"/>
        	<element name="orderItemRef" type="impl:ErpId" minOccurs="0">
        		<annotation>
        			<documentation>
        				Odkaz na radek objednavky, ktere se reklamace tyka.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="defectType" type="xsd:string" maxOccurs="1" minOccurs="0">
        		<annotation>
        			<documentation>
        				ciselnikova hodnota klasifikujici zavadu.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="defectDesc" type="xsd:string" minOccurs="0">
        		<annotation>
        			<documentation>Popis zavady.</documentation>
        		</annotation>
        	</element>
        	<element name="comment" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true">
        		<annotation>
        			<documentation>
        				Komentar pro zakaznika.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="resolutionComment" type="xsd:string" minOccurs="0">
        		<annotation>
        			<documentation>
        				Komentar k vyreseni reklamace.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="type" type="impl:ComplaintType" minOccurs="0" />
        	<element name="state" type="impl:ComplaintState"/>
        	<element name="dateCreated" type="xsd:dateTime" maxOccurs="1" minOccurs="0">
        		<annotation>
					<documentation>
						Datum vytvoreni reklamace na portale.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="dateAccepted" type="xsd:dateTime" minOccurs="0">
        		<annotation>
        			<documentation>
        				Datum akceptace reklamace na strane AX. Od totho
        				data se pocita zakonna lhuta na opravu vyrobku.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="dateFinished" type="xsd:dateTime" minOccurs="0">
        		<annotation>
        			<documentation>
        				Datum vyrizeni reklamace. Toto datum spolu s
        				datem akceptace udava dobu, jak dlouho se
        				reklamace provadela a o jakou se prodluzuje
        				zaruka na vyrobek.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="serialNumber" type="xsd:string" minOccurs="0">
        		<annotation>
        			<documentation>
        				Seriove cislo reklamovaneho zbozi. Toto pole
        				vyplnuje zakaznik, v AX muze dojit k jeho
        				oprave, nikoliv vymazani.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="shop" type="xsd:string" maxOccurs="1" minOccurs="0">
        		<annotation>
        			<documentation>
        				Identifikace provozovny spojene s reklamaci
        			</documentation>
        		</annotation>
        	</element>
        	<element name="contactEmail" type="xsd:string" maxOccurs="1" minOccurs="0">
        		<annotation>
        			<documentation>
        				Email na provozovnu, ktere reklamaci zastituje.
        				Email vyplni AX.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="repairShopAddress" type="impl:AddressCommon" maxOccurs="1" minOccurs="0">
        		<annotation>
        			<documentation>
        				Nazev a pripadne adresa servisu, ktery zarizeni opravuje.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="repairShopContacts" type="impl:Contacts" minOccurs="0" />
        	<element name="complainerAddress" type="impl:AddressCommon" maxOccurs="1" minOccurs="0" >
        		<annotation>
        			<documentation>
        				Informace o osobe, ktera reklamaci zadala 
        			</documentation>
        		</annotation>
        	</element>
        	<element name="complainerContacts" type="impl:Contacts" maxOccurs="1" minOccurs="0" />
        	<element name="replacementDevice" type="xsd:boolean" minOccurs="0">
        		<annotation>
        			<documentation>
        				Na zacatku prijem z ESH - AX na zaklade priznaku
        				vytvari SZ Zapujcka pri aktualizaci zp�t na
        				portal se odesila Ano, pokud zap�jcka dosud
        				nebyla uzavrena (neni na ni finalni transakce)
        			</documentation>
        		</annotation>
        	</element>
        	<element name="replacementDeviceId" type="impl:ErpId" minOccurs="0">
        		<annotation>
        			<documentation>
        				ERP ID pozadovaneho telefonu
        			</documentation>
        		</annotation>
        	</element>
        	<element name="replacementReturned" type="xsd:boolean" minOccurs="0">
        		<annotation>
        			<documentation>
        				Priznak, zda byl nahradni telefon vracen.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="maximalPrice" type="xsd:decimal" minOccurs="0">
        		<annotation>
        			<documentation>
        				Pro placene opravy stanovuje zakaznik maximalni
        				cenu opravy. Pokud predpokladane naklady na
        				opravu presahnou tuto castku, servis rovnou
        				vyrobek vraci.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="estimatedPrice" type="xsd:decimal" minOccurs="0">
        		<annotation>
        			<documentation>
        				Odhad ceny za opravu - nabizi servis zakaznikovi
        				ke schvaleni. Servis nesmi tuto cenu zmenit o
        				vice nez 10%.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="repairPrice" type="xsd:decimal" minOccurs="0" />
        	<element name="diagnosisPrice" type="xsd:decimal" minOccurs="0"/>
        	<element name="requiredResolution" type="impl:ComplaintResolution" minOccurs="0">
        		<annotation>
        			<documentation>
        				Pozadovany zpusob opravy. Vyplnuje zakaznik jako svou preferenci.
        			</documentation>
        		</annotation>
        	</element>
        	<element name="resolution" type="impl:ComplaintResolution" minOccurs="0">
        		<annotation>
        			<documentation>
        				Skutecny zpusob vyrizeni reklamace.
        			</documentation>
        		</annotation>
        	</element>
        	</sequence>
        	</extension>
		</complexContent>	
	</complexType>
	<element name="ComplaintArray" type="impl:ComplaintArray" />
	<complexType name="ComplaintArray">
		<sequence>
			<element name="eComplaint" type="impl:Complaint" minOccurs="0" maxOccurs="unbounded" />
		</sequence>
	</complexType>

	<!-- zbozi v katalogu -->
	<complexType name="Commodity">
		<complexContent>
			<extension base="impl:ErpObject">
				<sequence>
					<element name="parentRef" nillable="true" minOccurs="0" type="impl:ErpId"/> <!-- V aukcnim zbozi odkazuje na "originalni" produkt -->
					<element name="producerRef" nillable="true" minOccurs="0" type="impl:ErpId"/>
					<element name="segmentRef" nillable="true" minOccurs="0" type="impl:ErpId"/>
					<element name="title" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="description" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="keywords" nillable="true" minOccurs="0" type="impl:LangStringArray"/>
					<element name="ean" nillable="true" minOccurs="0" type="xsd:string"/>
					<element name="width" nillable="true" minOccurs="0" type="xsd:decimal"/>
					<element name="height" nillable="true" minOccurs="0" type="xsd:decimal"/>
					<element name="depth" nillable="true" minOccurs="0" type="xsd:decimal"/>
					<element name="weight" nillable="true" minOccurs="0" type="xsd:decimal"/>
					<element name="warranty" nillable="true" minOccurs="0" type="xsd:int"/>
					<element name="flagSet" nillable="true" minOccurs="0" type="impl:ListOfFlag"/>

					<!-- Primarni kategorie (primaryCategoryRef) musi byt zaroven v seznamu kategorii (categoryRefs) -->
					<element name="primaryCategoryRef" nillable="true" minOccurs="0" type="impl:ErpId"/>
					<element name="categoryRefs" nillable="true" minOccurs="0" type="impl:ErpIdArray"/>

					<element name="attributeList" nillable="true" minOccurs="0" type="impl:AttrValueArray"/>
					<element name="role" nillable="true" minOccurs="0" type="impl:CommodityRole"/>

					<element name="buyInEshop" nillable="false" minOccurs="1" maxOccurs="1" type="xsd:boolean"/> <!-- Zobrazeni tlacitka "Koupit" -->
					<element name="buyInAffiliates" nillable="false" minOccurs="1" maxOccurs="1" type="xsd:boolean"/> <!-- Zobrazeni tlacitka "Koupit v prodejne" -->
					<element name="checkAvailability" nillable="false" minOccurs="1" maxOccurs="1" type="xsd:boolean"/> <!-- Podminit zobrazeni tlacitek kontrolou dostupnosti? -->

					<element name="visible" nillable="false" minOccurs="1" maxOccurs="1" type="xsd:boolean"/> <!-- Je produkt na eshopu zobrazitelny? -->
					<element name="visibleInCategories" nillable="false" minOccurs="1" maxOccurs="1" type="xsd:boolean"/> <!-- Ma se produkt zobrazovat ve vypisu kategorii? -->
					<element name="fullDetail" nillable="false" minOccurs="1" maxOccurs="1" type="xsd:boolean"/> <!-- Zobrazovat detail produktu se vsemi informacemi nebo jen zakladnimi? -->
					<element name="snRequired" type="xsd:boolean" minOccurs="0" maxOccurs="1"/> <!-- Je vyzadovano seriove cislo Tj. je nutne komoditu v objednavce mit rozepsanou po kusech-->
					<element name="pddDate" type="xsd:dateTime" minOccurs="0" maxOccurs="1"/> <!-- datum naskladneni - pro SDA rezim-->
					<element name="pddMode" type="xsd:boolean" minOccurs="0" maxOccurs="1"/> <!-- pokud je true, je zbozi zobrazovano v rezimu SDA-->
					<element name="rchid" nillable="true" minOccurs="0" maxOccurs="1" type="xsd:string"/> <!-- reverse charge id-->
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="CommodityArray">
		<sequence>
			<element name="eCommodity" type="impl:Commodity" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- specialni druhy "zbozi": sluzby, bundly atp. -->
	<complexType name="CommBundle">
		<complexContent>
			<extension base="impl:Commodity"/>
		</complexContent>
	</complexType>
	<complexType name="CommAccessory">
		<complexContent>
			<extension base="impl:Commodity"/>
		</complexContent>
	</complexType>

	<!-- vazby mezi zbozim -->
	<complexType name="Relation" abstract="true">
		<sequence>
			<!-- Mozne hodnoty: ACC_NORMAL, ACC_GIFT, ACC_DISCOUNT, BUNDLE, COM_ITEM, SRV_NORMAL, ALTERNATIVE, SUCCESSOR, COMPETING -->
			<element name="relationType" nillable="true" minOccurs="0" type="impl:Identifier"/>
			<element name="relationId" nillable="true" minOccurs="0" type="impl:ErpId"/>
			<element name="commodity1" nillable="true" minOccurs="0" type="impl:ErpId"/>
			<element name="commodity2" nillable="true" minOccurs="0" type="impl:ErpId"/>
			<element name="position" nillable="true" minOccurs="0" type="xsd:int"/> <!-- Pozice zobrazeni skupiny prislusenstvi -->
			<element name="itemPosition" nillable="true" minOccurs="0" type="xsd:int"/> <!-- Pozice vyrobku ve skupine -->
			<element name="validFrom" nillable="true" minOccurs="0" type="xsd:dateTime"/>
			<element name="validTo" nillable="true" minOccurs="0" type="xsd:dateTime"/>
		</sequence>
		<attribute name="deleted" type="impl:RelDeleteMode" use="optional" default="DEL_NONE"/>
	</complexType>
	<complexType name="RelAccessory">
		<complexContent>
			<extension base="impl:Relation">
				<sequence>
					<element name="newPrice" nillable="true" minOccurs="0" type="impl:PriceData"/>
					<element name="priceType" nillable="true" minOccurs="0" type="impl:PriceType"/>
					<element name="unitQty" nillable="true" minOccurs="0" type="xsd:int"/>
					<element name="qtyLimit" nillable="true" minOccurs="0" type="impl:AccessoryLimit"/>
					<element name="supplemOrderLimit" nillable="true" minOccurs="0" type="xsd:int"/>
					<element name="groupId" nillable="true" minOccurs="0" type="impl:ErpId"/>
					<element name="groupLimit" nillable="true" minOccurs="0" type="xsd:int"/>
					<element name="hiddenInDetail" nillable="true" minOccurs="0" type="xsd:boolean"/>
					<element name="hiddenInFilter" nillable="true" minOccurs="0" type="xsd:boolean"/>
					<element name="mandatory" nillable="true" minOccurs="0" type="xsd:boolean"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="RelationArray">
		<sequence>
			<element name="eRelation" type="impl:Relation" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- axapta segmentace zbozi - rodina -->
	<complexType name="CommodityFamily">
		<complexContent>
			<extension base="impl:ErpObject">
				<sequence>
					<element name="description" nillable="true" minOccurs="0" type="xsd:string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="CommodityFamilyArray">
		<sequence>
			<element name="eCommodityFamily" type="impl:CommodityFamily" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- axapta segmentace zbozi - segment -->
	<complexType name="CommoditySegment">
		<complexContent>
			<extension base="impl:ErpObject">
				<sequence>
					<element name="commodityFamilyRef" nillable="true" minOccurs="0" type="impl:ErpId"/>
					<element name="description" nillable="true" minOccurs="0" type="xsd:string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="CommoditySegmentArray">
		<sequence>
			<element name="eCommoditySegment" type="impl:CommoditySegment" maxOccurs="unbounded" minOccurs="0"/>
		</sequence>
	</complexType>

	<!-- modelove urovne produktove rady -->
	<complexType name="ProductLineLevel">
		<sequence>
			<element name="title" type="impl:LangString"/>
			<element name="description" nillable="true" minOccurs="0" type="impl:LangString"/>
			<element name="commodityRef" nillable="true" type="impl:ErpId"/>
		</sequence>
	</complexType>
	<complexType name="ProductLineLevelArray">
		<sequence>
			<element name="eProductLineLevel" type="impl:ProductLineLevel" maxOccurs="unbounded" minOccurs="0"/>
		</sequence>
	</complexType>

	<!-- srovnavaci atributy produktove rady -->
	<complexType name="ProductLineAttribute">
		<sequence>
			<element name="title" type="impl:LangString"/>
			<element name="position" type="xsd:int"/>
			<element name="values" type="impl:ProductLineAttributeValueArray"/>
		</sequence>
	</complexType>
	<complexType name="ProductLineAttributeArray">
		<sequence>
			<element name="eProductLineAttribute" type="impl:ProductLineAttribute" maxOccurs="unbounded" minOccurs="0"/>
		</sequence>
	</complexType>

	<!-- hodnoty srovnavacich atributu produktove rady -->
