		</complexContent>
	</complexType>
	<complexType name="MicroRegionArray">
		<sequence>
			<element name="eMicroRegion" type="impl:MicroRegion" maxOccurs="unbounded" minOccurs="0"/>
		</sequence>
	</complexType>

	<!-- vazba slotu na dorucovaci region -->
	<complexType name="SlotForRegion">
		<sequence>
			<element name="deliveryRegionRef" nillable="true" type="impl:ErpId"/>
			<element name="timeSlotDateRef" nillable="true" type="impl:ErpId"/>
		</sequence>
		<attribute name="deleted" type="impl:RelDeleteMode" use="optional" default="DEL_NONE"/>
	</complexType>
	<complexType name="SlotForRegionArray">
		<sequence>
			<element name="eSlotForRegion" type="impl:SlotForRegion" maxOccurs="unbounded" minOccurs="0"/>
		</sequence>
	</complexType>

	<!-- vazba PSC na mikroregiony -->
	<complexType name="ZipCode">
		<sequence>
			<element name="zipCode" type="xsd:string"/>
			<element name="city" type="xsd:string"/>
			<element name="microRegionRef" nillable="true" type="impl:ErpId"/>
		</sequence>
		<attribute name="deleted" type="xsd:boolean" use="optional" default="false"/>
	</complexType>
	<complexType name="ZipCodeArray">
		<sequence>
			<element name="eZipCode" type="impl:ZipCode" maxOccurs="unbounded" minOccurs="0"/>
		</sequence>
	</complexType>


	<!-- ========== compcare ========== -->

	<!-- stav EAN poukazky -->
	<simpleType name="EANVoucherState">
		<restriction base="xsd:string">
			<enumeration value="UNASSIGNED" />
			<enumeration value="PAID" />
			<enumeration value="TERMINATED" />
			<enumeration value="SUSPENDED" />
			<enumeration value="ACTIVATED" />
		</restriction>
	</simpleType>

	<!-- EAN poukazka -->
	<complexType name="EANVoucher">
		<sequence>
			<element name="commodityRef" nillable="true" minOccurs="0" type="impl:ErpId"/>
			<element name="eanCode" type="xsd:string"/>
			<element name="pinCode" type="xsd:string"/>
			<element name="state" type="impl:EANVoucherState"/>
			<element name="email" nillable="true" type="xsd:string"/>
		</sequence>
		<attribute name="deleted" type="xsd:boolean" use="optional" default="false"/>
	</complexType>
	<complexType name="EANVoucherArray">
		<sequence>
			<element name="eEANVoucher" type="impl:EANVoucher" maxOccurs="unbounded" minOccurs="0"/>
		</sequence>
	</complexType>


	<!-- ========== slevove kupony ========== -->

	<!-- stav slevoveho kuponu -->
	<simpleType name="VoucherState">
		<restriction base="xsd:string">
			<enumeration value="ACTIVATED" />
			<enumeration value="SUSPENDED" />
		</restriction>
	</simpleType>

	<!-- slevove kupony -->
	<complexType name="Voucher">
		<sequence>
			<element name="code" type="xsd:string"/>
			<element name="validFrom" nillable="true" minOccurs="0" type="xsd:dateTime"/>
			<element name="validTo" nillable="true" minOccurs="0" type="xsd:dateTime"/>
			<element name="state" type="impl:VoucherState"/>
		</sequence>
		<attribute name="deleted" type="xsd:boolean" use="optional" default="false"/>
	</complexType>

	<complexType name="VoucherArray">
		<sequence>
			<element name="eVoucher" type="impl:Voucher" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<simpleType name="DiscountType">
		<restriction base="xsd:string">
			<enumeration value="ABSOLUTE" /> <!-- Sleva je absolutni - v korunach -->
			<enumeration value="RATIO" /> <!-- Pomerna sleva -->
		</restriction>
	</simpleType>

	<complexType name="DiscountVoucher">
		<complexContent>
			<extension base="impl:Voucher">
				<sequence>
					<element name="discountType" type="impl:DiscountType"/>
					<element name="discount" type="xsd:decimal"/> <!-- Pro discountType = ABSOLUTE je sleva v korunach, pro RATIO je to pomer (v rozsahu 0.0 - 1.0) -->
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="GiftVoucher">
		<complexContent>
			<extension base="impl:Voucher">
				<sequence>
					<element name="pin" type="xsd:string"/>
					<element name="amount" type="xsd:decimal"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PromotionCode">
		<complexContent>
			<extension base="impl:Voucher">
				<sequence>
					<element name="commodityRef" type="impl:ErpId"/>
					<element name="discountType" type="impl:DiscountType"/>
					<element name="discount" type="xsd:decimal"/> <!-- Pro discountType = ABSOLUTE je sleva v korunach, pro RATIO je to pomer (v rozsahu 0.0 - 1.0) -->
					<element name="singleUse" type="xsd:boolean"/> <!-- Jednorazovy promotion code, po pouziti se deaktivuje -->
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<!-- ========== navody a manualy ========== -->

	<!-- navody k produktum -->
	<complexType name="CommodityManual">
		<sequence>
			<element name="id" type="impl:Identifier"/>
			<element name="path" type="xsd:string"/>
			<element name="itemRef" type="impl:Identifier"/>
			<element name="description" type="xsd:string"/>
			<element name="fileSize" type="xsd:string"/>
			<element name="language" type="xsd:string"/>
			<!--
				Hodnoty:
					ES - energeticky stitek
					IL - informacni list
					MA - navod
			 -->
			<element name="type" type="xsd:string"/>
		</sequence>
		<attribute name="deleted" type="xsd:boolean" use="optional" default="false"/>
	</complexType>
	<complexType name="CommodityManualArray">
		<sequence>
			<element name="eManual" type="impl:CommodityManual" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>


	<!-- ========== zmena vlastnika objednavky ========== -->

	<!-- request zmeny vlastnika -->
	<complexType name="OrderOwnerChangeData">
		<sequence>
			<element name="orderId" type="impl:Identifier"/>
			<element name="totalPrice" type="xsd:decimal"/>
			<element name="userRef" type="impl:Identifier"/>
		</sequence>
	</complexType>

	<!-- ========== marketingove pobidky ========== -->

	<!-- stav marketingove pobidky -->
	<simpleType name="MarketingOfferState">
		<restriction base="xsd:string">
			<enumeration value="ACTIVE"/>
			<enumeration value="SUSPENDED"/>
		</restriction>
	</simpleType>

	<!-- typ polozky marketingove pobidky -->
	<simpleType name="MarketingOfferItemType">
		<restriction base="xsd:string">
			<enumeration value="COMMODITY"/>
			<enumeration value="CATEGORY"/>
			<enumeration value="ASSET"/>
		</restriction>
	</simpleType>

	<!-- polozka marketingove pobidky -->
	<complexType name="MarketingOfferItem">
		<sequence>
			<element name="itemType" type="impl:MarketingOfferItemType"/>
			<element name="itemId" type="impl:Identifier"/>
		</sequence>
	</complexType>

	<!-- definice marketingove pobidky -->
	<complexType name="MarketingOffer">
		<sequence>
			<element name="id" type="impl:Identifier"/>
			<element name="title" type="xsd:string"/>
			<element name="text" type="xsd:string"/>
			<element name="validFrom" type="xsd:dateTime"/>
			<element name="validTo" type="xsd:dateTime"/>
			<element name="item" type="impl:MarketingOfferItem"/>
			<element name="state" type="impl:MarketingOfferState"/>
		</sequence>
		<attribute name="deleted" type="xsd:boolean" use="optional" default="false"/>
	</complexType>
	<complexType name="MarketingOfferArray">
		<sequence>
			<element name="eOffer" type="impl:MarketingOffer" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<!-- adresat marketingove pobidky -->
	<complexType name="MarketingOfferRecipient">
		<sequence>
			<element name="offerRef" type="impl:Identifier"/>
			<element name="userRef" type="impl:Identifier"/>
		</sequence>
		<attribute name="deleted" type="xsd:boolean" use="optional" default="false"/>
	</complexType>
	<complexType name="MarketingOfferRecipientArray">
		<sequence>
			<element name="eRecipient" type="impl:MarketingOfferRecipient" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
<!--  ============== externi pobocky PNS/PONTY/... ============ -->
	<!-- pobocky -->
	<complexType name="Branch">
		<complexContent>
			<extension base="impl:ErpObject">
				<sequence>
						<element name="branchType" type="xsd:string"/>	<!-- typ vydejniho mista PNS/PONTY -->
						<element name="branch" type="xsd:string"/>     	<!-- pobocka PNS1,... -->
						<element name="place" type="xsd:int"/>   		<!-- vydejni misto -->
						<element name="title" type="xsd:string"/>
						<element name="street"  nillable="true" minOccurs="0" type="xsd:string"/>
						<element name="city" nillable="true" minOccurs="0" type="xsd:string"/>
						<element name="zip" type="xsd:string"/>
						<element name="country" nillable="true" type="xsd:string"/>
						<element name="latitude" nillable="true" minOccurs="0" type="xsd:string"/>
						<element name="longitude" nillable="true" minOccurs="0" type="xsd:string"/>
						<element name="phone" nillable="true" minOccurs="0" type="xsd:string"/>
						<element name="email" nillable="true" minOccurs="0" type="xsd:string"/>
						<element name="canceled" type="xsd:boolean"/>
						<element name="closedFrom" nillable="true" minOccurs="0" type="xsd:dateTime"/>
						<element name="closedTo" nillable="true" minOccurs="0" type="xsd:dateTime"/>
						<element name="openingHours" nillable="true" minOccurs="0" type="xsd:string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="BranchArray">
		<sequence>
			<element name="eBranch" type="impl:Branch" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
	<!--  ============== pracovni doba prodavacu ============ -->
	<complexType name="EmployeeWorkingHours">
		<complexContent>
			<extension base="impl:ErpObject">
				<sequence>
						<element name="employeeId" type="impl:Identifier"/>
						<element name="shopId" type="impl:Identifier"/>
						<element name="date" type="xsd:date"/>
						<element name="workingHours" type="xsd:string"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="EmployeeWorkingHoursArray">
		<sequence>
			<element name="eEmployeeWorkingHours" type="impl:EmployeeWorkingHours" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>

</schema>
