	</complexType>
	<complexType name="ProductLineAttributeValueArray">
		<sequence>
			<element name="eProductLineAttributeValue" type="impl:ProductLineAttributeValue" maxOccurs="unbounded" minOccurs="0"/>
		</sequence>
	</complexType>

	<!-- produktove rady -->
	<complexType name="ProductLine">
		<complexContent>
			<extension base="impl:ErpObject">
				<sequence>
					<element name="title" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="description" nillable="true" minOccurs="0" type="impl:LangString"/>
					<element name="validFrom" nillable="true" minOccurs="0" type="xsd:dateTime"/>
					<element name="validTo" nillable="true" minOccurs="0" type="xsd:dateTime"/>
					<element name="levels" nillable="true" minOccurs="0" type="impl:ProductLineLevelArray"/>
					<element name="attributes" nillable="true" minOccurs="0" type="impl:ProductLineAttributeArray"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ProductLineArray">
		<sequence>
			<element name="eProductLine" type="impl:ProductLine" maxOccurs="unbounded" minOccurs="0"/>
